{
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": [
        "src/",
        "vendor/composer"
    ],
    "extract": false,
    "files": [
        "vendor/autoload.php",
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [{
        "name": "*.php",
        "exclude": [
            "behat",
            "mikey179",
            "Net",
            "phpunit",
            "phpunit-test-case",
            "Tester",
            "silex",
            "yaml",
            "tests",
            "Tests",
            "Tester",
            "symfony/debug",
            "symfony/event-dispatcher",
            "symfony/dependency-injection",
            "symfony/finder",
            "symfony/http-foundation",
            "symfony/http-kernel",
            "symfony/options-resolver",
            "symfony/process",
            "symfony/routing",
            "symfony/translation"
        ],
        "in": [
            "vendor/guzzlehttp",
            "vendor/symfony/config",
            "vendor/symfony/console",
            "vendor/symfony/filesystem",
            "vendor/symfony/yaml",
            "vendor/herrera-io",
            "vendor/seld/jsonlint",
            "vendor/justinrainbow/json-schema",
            "vendor/kherge/version",
            "vendor/silvadanilo"
        ]
    }],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "kbize.php",
    "output": "bin/kbize.phar",
    "replacements": {
        "manifest_url": "http://box-project.org/manifest.json"
    },
    "stub": true
}
